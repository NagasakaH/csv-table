{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist/",
    "strict": true,
    "plugins": [{"name": "typescript-plugin-css-modules"}],
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "**/__tests__/*", "dist", "sample", "webpack"]
}
